(* vim: ft=antlr
*)

%tokens
  (* punctuation *)
  : LParen ("(") | RParen (")")
  | LSquare ("[") | RSquare ("]")
  | LCurly ("{") | RCurly ("}")
  | Quote ("'") | Backtick ("`")
  | Splice ("~@")
  | Tilde ("~")
  | At ("@")
  | Caret ("^")
    (* comments *)
  | CommentStart (";") | CommentEnd ("\n")
    (* regular program text *)
  | DoubleQuote ("\"")
  | Char of char
  | Num of int
  | True ("true") | False ("false")
  | Nil ("nil")
;

%start form;
